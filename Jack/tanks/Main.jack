class Main
{
	function void main() 
	{
		// create a new tank game
		var TankGame tankGame;
		let tankGame = TankGame.new();

		// and enter game loop
		do tankGame.run();

		// done, so clean up everything
		do tankGame.dispose();

		return;
	}

	function void printString(String str)
	{
		do Output.printString(str);
		do str.dispose();
		return;
	}
}
